<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <div class="card-social social-line" data-buttons="3">
      <%= link_to "Se connecter avec #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider), class: 'social-share-button' %><br/>
    </div>
  <% end -%>
<% end -%>
